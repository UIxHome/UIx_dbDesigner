{
  "name": "uix_dbdesigner",
  "version": "1.0.0",
  "description": "An Electron application to design relationship between tables",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UIxHome/UIx_dbDesigner.git"
  },
  "keywords": [
    "dbDesigner",
    "database",
    "relationship"
  ],
  "author": "UIxHome.fr",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/UIxHome/UIx_dbDesigner/issues"
  },
  "homepage": "https://github.com/UIxHome/UIx_dbDesigner#readme",
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "d3-shape": "^3.2.0",
    "fs": "^0.0.1-security",
    "konva": "^9.3.16",
    "luxon": "^3.5.0",
    "pinia": "^2.3.0",
    "vue-konva": "^3.2.0",
    "vuetify": "^3.7.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.10.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "electron": "33.3.0",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.3.2",
    "vite": "^5.3.1",
    "vue": "^3.5.13",
    "vue-svg-loader": "^0.16.0"
  }
}
